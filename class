#CLASS

class Student:
    def say_hello(self):
        print("Hi, I'm a student!")
s1 = Student()
s1.say_hello()



#EXAMPLE

class Students:
    def __init__(self,name,grade):
        self.name = name
        self.grade = grade
    def display(self):
        print(f"{self.name} is in grade {self.grade}")
s2 = Students("Ajmal",10)
s3 = Students("Gani",11)
s4 = Students("Priya",12)
s2.display()
s3.display()
s4.display()
